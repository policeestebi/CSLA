/* ----------------------------------------
   Modal PopUp
------------------------------------------- */
.modalPopup {
	
	background-color:#ffffff;
	border-width:3px;
	border-style:solid;
	border-color:Gray;
	padding:3px;
	width:250px;
}

.modalPopupHead {
	cursor: move;
	background-color:#DDDDDD;
	border:solid 1px Gray;
	color:Black
}

.modalBackground {
	background-color:Gray;
	/*filter:alpha(opacity=70);
	opacity:0.5;*/
}

